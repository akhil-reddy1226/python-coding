#Hello World
print("Hello World")

#Hello World with tab
print("Hello World hello world")

#Hello World with new line
print("hello world")
print("hello world")

#Integer Input
a=int(input())
print(a)

#float input
a=float(input())
print("%2f"%a)

#character input
a=input()
print(a)

#string input
a=str(input())
print(a)

#ascii-1
a=input()
print(ord(a))

ascii-2
a=int(input())
print(chr(a))
